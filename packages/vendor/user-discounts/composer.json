{
    "name": "vendor/user-discounts",
    "description": "A Laravel package for managing user-level discounts",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\UserDiscounts\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\UserDiscounts\\UserDiscountsServiceProvider"
            ],
            "aliases": {
                "Discount": "Vendor\\UserDiscounts\\Facades\\Discount"
            }
        }
    }
}